stylesheet OurTaxFreeSolution {
  page Taxes {
    section "Meta" {
        question likesToPayTaxes widget checkbox
        question likesToPayTaxes widget radio ( "Radio" , "Television" )
    }
  }
  page Income {
    section "Beta" {
        question likesToPayTaxes widget radio ( "yes" , "no" )
    }
    section "Gamma" {
        question likesToPayTaxes widget dropdown ( "isTrue" , "isFalse " )
    }
  }
}
