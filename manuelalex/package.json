{
  "name": "manuelalex",
  "version": "0.0.1",
  "description": "Software Construction - QL & QSL",
  "main": "index.js",
  "scripts": {
    "test": "mocha --compilers js:babel-core/register --recursive --timeout 15000"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/software-engineering-amsterdam/myriad-ql.git"
  },
  "keywords": [
    "Software",
    "Construction",
    "QL",
    "QSL"
  ],
  "author": "Manuel Overdijk, Alex van Manen",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/software-engineering-amsterdam/myriad-ql/issues"
  },
  "homepage": "https://github.com/software-engineering-amsterdam/myriad-ql#readme",
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-preset-es2015": "^6.22.0",
    "babel-register": "^6.22.0",
    "chai": "^3.5.0",
    "eslint": "^3.15.0",
    "jspm": "0.17.0-beta.31",
    "sinon": "^1.17.7",
    "mocha": "^3.2.0"
  },
  "jspm": {
    "name": "manuelalex",
    "main": "main.js",
    "dependencies": {
      "babel-register": "npm:babel-register@^6.22.0"
    },
    "devDependencies": {
      "plugin-babel": "npm:systemjs-plugin-babel@^0.0.20"
    },
    "peerDependencies": {
      "assert": "npm:jspm-nodelibs-assert@^0.2.0",
      "buffer": "npm:jspm-nodelibs-buffer@^0.2.0",
      "child_process": "npm:jspm-nodelibs-child_process@^0.2.0",
      "constants": "npm:jspm-nodelibs-constants@^0.2.0",
      "crypto": "npm:jspm-nodelibs-crypto@^0.2.0",
      "events": "npm:jspm-nodelibs-events@^0.2.0",
      "fs": "npm:jspm-nodelibs-fs@^0.2.0",
      "module": "npm:jspm-nodelibs-module@^0.2.0",
      "os": "npm:jspm-nodelibs-os@^0.2.0",
      "path": "npm:jspm-nodelibs-path@^0.2.0",
      "process": "npm:jspm-nodelibs-process@^0.2.0",
      "stream": "npm:jspm-nodelibs-stream@^0.2.0",
      "string_decoder": "npm:jspm-nodelibs-string_decoder@^0.2.0",
      "util": "npm:jspm-nodelibs-util@^0.2.0",
      "vm": "npm:jspm-nodelibs-vm@^0.2.0"
    },
    "overrides": {
      "npm:debug@2.6.0": {
        "main": "src/browser.js",
        "jspmNodeConversion": false,
        "format": "cjs",
        "map": {
          "./src/browser.js": {
            "node": "./src/node.js"
          },
          "./node.js": {
            "browser": "./src/browser.js"
          },
          "fs": "@node/fs",
          "net": "@node/net",
          "tty": "@node/tty",
          "util": "@node/util"
        }
      },
      "npm:inherits@2.0.3": {
        "ignore": [
          "test.js"
        ]
      },
      "npm:lodash@4.17.4": {
        "map": {
          "buffer": "@empty",
          "process": "@empty"
        }
      },
      "npm:ms@0.7.2": {
        "jspmNodeConversion": false,
        "format": "cjs"
      }
    }
  }
}
